 <openerp>
 <data>
 <!-- Sequences for Credit Details -->
        <record id="seq_type_credit_details" model="ir.sequence.type">
            <field name="name">Credit Details</field>
            <field name="code">credit.details</field>
        </record>
        <record id="seq_credit_details" model="ir.sequence">
            <field name="name">Credit Details</field>
            <field name="code">credit.details</field>
            <field name="prefix">CRD</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
                
        <!-- Sequences for Credit Limit Details -->
        <record id="seq_type_credit_limit_details" model="ir.sequence.type">
            <field name="name">Credit Limit Details</field>
            <field name="code">credit.limit.details</field>
        </record>
        <record id="seq_credit_limit_details" model="ir.sequence">
            <field name="name">Credit Limit Details</field>
            <field name="code">credit.limit.details</field>
            <field name="prefix">CLD</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Sequences for Blocking/Unblocking Customer -->
        <record id="seq_type_block_customer" model="ir.sequence.type">
            <field name="name">block_customer_sequence</field>
            <field name="code">block.customer</field>
        </record>
        <record id="seq_block_customer" model="ir.sequence">
            <field name="name">block_customer</field>
            <field name="code">block.customer</field>
            <field name="prefix">BL</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <record id="seq_type_customer_blocking" model="ir.sequence.type">
            <field name="name">customer_blocking_sequence</field>
            <field name="code">customer.blocking.no</field>
       	 </record>
          <record id="seq_customer_blocking_no" model="ir.sequence">
            <field name="name">customer_blocking_no</field>
            <field name="code">customer.blocking.no</field>
            <field name="prefix">CB</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
</data>
</openerp>